@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<h1 class="display-4">Post a song</h1>

<form method="post">
    @Html.LabelFor(a => a.BundleMusic.Artist)
    <input asp-for="BundleMusic.Artist" required class="form-control">
    <br />
    @Html.LabelFor(a => a.BundleMusic.Title)
    <input asp-for="BundleMusic.Title" required class="form-control">
    <br />
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
